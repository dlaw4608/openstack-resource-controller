---
apiVersion: kuttl.dev/v1beta1
kind: TestAssert
resourceRefs:
    - apiVersion: openstack.k-orc.cloud/v1alpha1
      kind: {{ .Kind }}
      name: {{ .PackageName }}-import-external
      ref: {{ .PackageName }}1
    - apiVersion: openstack.k-orc.cloud/v1alpha1
      kind: {{ .Kind }}
      name: {{ .PackageName }}-import-external-not-this-one
      ref: {{ .PackageName }}2
assertAll:
    - celExpr: "{{ .PackageName }}1.status.id != {{ .PackageName }}2.status.id"
---
apiVersion: openstack.k-orc.cloud/v1alpha1
kind: {{ .Kind }}
metadata:
  name: {{ .PackageName }}-import
status:
  conditions:
    - type: Available
      message: OpenStack resource is available
      status: "True"
      reason: Success
    - type: Progressing
      message: OpenStack resource is up to date
      status: "False"
      reason: Success
  resource:
    name: {{ .PackageName }}-import-external
    description: {{ .Kind }} {{ .PackageName }}-import-external from "{{ .PackageName }}-import" test
    # TODO(scaffolding): Add all fields the resource supports
